{
	"id": "com.sample.box.opacity",
	"version": "1.0.0",
	"name": "Colored Box",
	"description": "Box",
	"newInstancePrefix": "Box",
	"icon": "https://DB-Off.github.io/SAC---Customwidgets/box/icon.png",
	"vendor": "Sample",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sample-box",
			"url": "https://DB-Off.github.io/SAC---Customwidgets/box/box.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-sample-box-builder",
			"url": "https://DB-Off.github.io/SAC---Customwidgets/box/box_builder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 0.5
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setColor": {
			"description": "Sets the background color.",
			"parameters": [
				{
					"name": "newColor",
					"type": "string",
					"description": "The new background color"
				}
			],
			"body": "this.setColor(newColor);"
		},
		"getColor": {
			"returnType": "string",
			"description": "Returns the background color.",
			"body": "return this.getColor();"
		}
		"setOpacity": {
			"parameters": ["opacity"],
			"body": "this.opacity = opacity;"
		}
		"getOpacity": {
			"parameters": [],
			"body": "return this.opacity;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}












